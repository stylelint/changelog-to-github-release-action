{
  "name": "changelog-to-github-release-action",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub Action to convert CHANGELOG to GitHub Release",
  "repository": "ybiquitous/changelog-to-github-release-action",
  "license": "MIT",
  "author": "Masafumi Koba",
  "type": "module",
  "scripts": {
    "build": "ncc build src/main.ts",
    "format": "prettier --write --cache .",
    "lint": "npm-run-all --parallel --continue-on-error lint:*",
    "lint:formatting": "prettier . --check --cache",
    "lint:types": "tsc",
    "pretest": "npm run lint",
    "test": "vitest run && npm run build",
    "watch": "vitest watch"
  },
  "prettier": "@stylelint/prettier-config",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "remark": "^14.0.3",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@stylelint/prettier-config": "^3.0.0",
    "@tsconfig/node16": "^16.1.0",
    "@tsconfig/strictest": "^2.0.1",
    "@types/mdast": "^4.0.0",
    "@vercel/ncc": "^0.36.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vitest": "^0.33.0"
  },
  "engines": {
    "node": ">=16"
  }
}
